package DogPoundBreakout;

import java.util.Vector;

/**
 * Additional vector tools and utilities
 * @author Rob Bauer
 *
 */
public class VectorTools extends Vector {
	
	/**
	 * @param nSize initial vector size
	 */
	public VectorTools(String sString, int nSize) {
		super(nSize);								// initialize the vector via the super constructor
		
		// build all possible permutations of sString - add each one back to this vector
		buildPermutation(sString);
	}  // end public static VectorTools(int nSize) {
	
	
}  // end public class VectorTools extends Vector
